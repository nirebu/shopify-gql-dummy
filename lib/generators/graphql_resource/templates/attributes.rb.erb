# frozen_string_literal: true

class <%= class_name.camelize %>
  module Attributes
    extend ActiveSupport::Concern

    included do
      include ActiveModel::Model

      attr_accessor(
      <%- config[:attributes].each_with_index do |attribute, index| -%>
        <%= attribute %><%= "," if index != (config[:attributes].size - 1) %>
      <%- end -%>
      )
    end
  end
end
